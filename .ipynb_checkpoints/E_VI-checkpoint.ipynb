   ],
   "source": [
    "def pruebaRango(q,n,m):\n",
    "    if n > m:\n",
    "        r = range(m,n)\n",
    "    else:\n",
    "        r = range(n,m)\n",
    "    res = q in r\n",
    "    return res\n",
    "\n",
    "print(pruebaRango(5,3,-9))"
   ]
